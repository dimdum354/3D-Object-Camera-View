Dim pmatay# = 0 
Dim pmataz# = 4

'koordinat titik pandang atau posisi titik pandang 
Dim ppandangx# = 0
Dim ppandangy# = 0
Dim ppandangz# = -4

'kecepatan transisi perpindahan
Dim pindah# = 0.01   

'variabel untuk menyimpan gambar tekstur pada bidang
Dim tekstur1
Dim tekstur2

tekstur1 = loadmipmaptexture("gambar/tekstur1.jpg")
tekstur2 = loadmipmaptexture("gambar/tekstur2.jpg")

'fungsi untuk mengaktifkan gambar tekstur pada bidang
glEnable(gl_texture_2d)

while true 
glClear(GL_COLOR_BUFFER_BIT or GL_DEPTH_BUFFER_BIT) 'untuk menghapus window dan menyesuaikan tampilan sesuai yang telah didefinisikan
glLoadIdentity() 'digunakan untuk memanggil matriks identitas dan dijadikan status matriks (proyeksi) saat ini

'untuk mendefiniskan garis pandang
