swapBuffers() 'digunakan untuk menukar bagian belakang buffer menjadi buffer layar (screen buffer)

if scankeydown(VK_RIGHT) then 'menggerakkan objek melalui keyboard
    pmatax# = pmatax# + pindah#
	'ppandangx# = ppandangx# + pindah#
elseif scankeydown(VK_LEFT) then 'menggerakkan objek melalui keyboard
    pmatax# = pmatax# - pindah# 
	'ppandangx# = ppandangx# - pindah#
elseif scankeydown(VK_UP) then 'menggerakkan objek melalui keyboard
	pmatay# = pmatay# + pindah#
	'ppandangy# = ppandangy# + pindah#
elseif scankeydown(VK_DOWN) then 'menggerakkan objek melalui keyboard
	pmatay# = pmatay# - pindah#
	'ppandangy# = ppandangy# - pindah#
elseif scankeydown(VK_NUMPAD8) then 'menggerakkan objek melalui keyboard
	pmataz# = pmataz# + pindah#
	'ppandangz# = ppandangz# + pindah#
elseif scankeydown(VK_NUMPAD2) then 'menggerakkan objek melalui keyboard
	pmataz# = pmataz# - pindah#
	'ppandangz# = ppandangz# - pindah#
endif

